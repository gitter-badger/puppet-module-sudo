{
  "name": "justinclayton-sudo",
  "version": "1.1.0",
  "source": "http://github.com/justinclayton/puppet-module-sudo.git",
  "author": "justinclayton",
  "license": "MIT",
  "summary": "A very flexible and well-tested puppet module for sudo",
